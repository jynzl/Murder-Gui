local notifSound = Instance.new("Sound",workspace)
notifSound.PlaybackSpeed = 1.5
notifSound.Volume = 0
notifSound.SoundId = "rbxassetid://170765130"
notifSound.PlayOnRemove = true
notifSound:Destroy()
game.StarterGui:SetCore("SendNotification", {Title = "No Game Detected", Text = "Still checking", Icon = "rbxassetid://", Duration = 5, Button1 = "Okay"})

-- murder
if game.PlaceID == 4572253581 then
loadstring(game:HttpGet(('https://raw.githubusercontent.com/jynzl/Murder-Gui/main/Murder%20Script'),true))()

local notifSound = Instance.new("Sound",workspace)
notifSound.PlaybackSpeed = 1.5
notifSound.Volume = 0.15
notifSound.SoundId = "rbxassetid://170765130"
notifSound.PlayOnRemove = true
notifSound:Destroy()
game.StarterGui:SetCore("SendNotification", {Title = "Game Found", Text = "Murder", Icon = "rbxassetid://505845268", Duration = 5, Button1 = "Okay"})
end
